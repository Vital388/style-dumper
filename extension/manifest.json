{
  "name": "Style Dumper",
  "version": "0.2.0",
  "manifest_version": 3,
  "description": "Extract CSS styles of selected element(s) from the page via DevTools.",
  "devtools_page": "devtools.html",
  "icons": {"128": "icon128.png"},
  "minimum_chrome_version": "88",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}